+int
+dhd_check(void *dhd)
+{
+	int ret;
+	int val;
+
+	if ((ret = dhd_get(dhd, DHD_GET_MAGIC, &val, sizeof(int)) < 0))
+		return ret;
+	if (val != DHD_IOCTL_MAGIC)
+		return -1;
+	if ((ret = dhd_get(dhd, DHD_GET_VERSION, &val, sizeof(int)) < 0))
+		return ret;
+	if (val > DHD_IOCTL_VERSION) {
+		fprintf(stderr, "Version mismatch, please upgrade\n");
+		return -1;
+	}
+	return 0;
+}
