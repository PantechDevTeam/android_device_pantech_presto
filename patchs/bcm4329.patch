+static int
+file_size(char *fname)
+{
+	FILE *fp;
+	long size = -1;
+
+	/* Can't use stat() because of Win CE */
+
+	if ((fp = fopen(fname, "rb")) == NULL ||
+	    fseek(fp, 0, SEEK_END) < 0 ||
+	    (size = ftell(fp)) < 0)
+		fprintf(stderr, "Could not determine size of %s: %s\n",
+		        fname, strerror(errno));
+
+	if (fp != NULL)
+		fclose(fp);
+
+	return (int)size;
